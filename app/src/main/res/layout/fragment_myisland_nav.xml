<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="72dp"
    tools:context="com.edge.work.travelbox.MyislandNavFragment"
    android:elevation="4dp"
    android:background="#FFFFFF">

<ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/myisland_nav_friend"
        android:id="@+id/btn_myisland_nav_friend"
        android:layout_alignParentTop="false"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/myisland_nav_share"
        android:id="@+id/btn_myisland_nav_share"
        android:layout_alignParentTop="false"
        android:layout_toRightOf="@+id/btn_myisland_nav_friend"
        android:layout_toEndOf="@+id/btn_myisland_nav_friend"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/myisland_nav_build"
        android:id="@+id/btn_myisland_nav_build"
        android:layout_alignParentTop="false"
        android:layout_toLeftOf="@+id/btn_myisland_nav_friend"
        android:layout_toStartOf="@+id/btn_myisland_nav_friend"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="8dp" />


</RelativeLayout>
